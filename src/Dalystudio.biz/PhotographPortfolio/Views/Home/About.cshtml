<!-- HOME -->
<section class="module module-header bg-dark bg-dark-50" data-background="../img/Photographer.jpg">

    <div class="container">

        <!-- MODULE TITLE -->
        <div class="row">

            <div class="col-sm-6 col-sm-offset-3">

                <h1 class="module-title font-alt align-center">About</h1>

