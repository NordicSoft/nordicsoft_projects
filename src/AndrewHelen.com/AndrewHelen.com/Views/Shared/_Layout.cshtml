    <script type="application/ld+json">
        {
        "@@context": "http://schema.org",
        "@@type": "Event",
        "name": "Andrew & Helen",
        "startDate": "2019-11-16T10:00",
        "endDate": "2019-11-16T23:00",
        "location": {
        "@@type": "Place",
        "name": "St. Monica Catholic Church",
        "address": {
        "@@type": "PostalAddress",
        "streetAddress": "701 California Ave",
        "addressLocality": "Santa Monica",
        "postalCode": "90403"
        }
        },
        "image": "https://andrew-helen.com/img/andrew_helen_google_image.jpg",
        "description": "Come and help us make our most important day unforgettable, find more details about us and our happy occasion here."
        }
    </script>
    <script src="~/dist/vendor_src.js" defer></script>
    <script src="~/dist/main.js" defer></script>

